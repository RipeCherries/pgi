{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-plusplus": "off",
        "no-bitwise": "off",
        "no-alert": "off",
        "no-undef": "off",
        "no-unused-vars": "off",
        "no-param-reassign": "off"
    }
}
